// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Newsletters {
  id        Int      @id @default(autoincrement())
  blogId    String
  messageId String
  email     String
  batchId   String
  opened    Float
  sent      Float
  submitted Float
  created   DateTime @default(now())
}
