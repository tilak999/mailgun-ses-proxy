{
  "name": "mailgun-to-ses-proxy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "next:build": "next build --webpack",
    "build": "prisma generate && next build --webpack && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production prisma migrate deploy && node dist/server.js",
    "lint": "next lint",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.981.0",
    "@aws-sdk/client-sqs": "^3.981.0",
    "@prisma/adapter-mariadb": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "next": "^16.1.6",
    "pino": "^10.3.0",
    "prisma": "^7.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.19",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
